                    <l xml:id="l-47r-2-6"><choice><abbr>Ꝑ</abbr><expan resp="#SDM">P<ex>ar</ex></expan></choice> <choice><orig>dieu</orig><reg resp="#SDM">Dieu</reg></choice> fame neſt pas voiſeuſe</l>
                    <l xml:id="l-47r-2-7"><choice><abbr>Qⁱ</abbr><expan resp="#SDM">Q<ex>ui</ex></expan></choice> des vvide</l>
                    <l xml:id="l-47r-2-8">Helas ma bourſe eſt toute vvide</l>
                    <l xml:id="l-47r-2-9">De<choice><abbr>mōnoie</abbr><expan resp="#SDM">mo<ex>n</ex>noie</expan></choice></l>
                    <l xml:id="l-47r-2-10">Maleureus eſt <choice><abbr>qͥ</abbr><expan resp="#SDM">q<ex>ui</ex></expan></choice> ſe noie</l>
                    <l xml:id="l-47r-2-11">De ſon gré</l>
